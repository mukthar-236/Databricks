						"value": "[parameters('metadatasoucelink_properties_typeProperties_url')]"
					},
					"sink_data_properties_typeProperties_url": {
						"value": "[parameters('sink_data_properties_typeProperties_url')]"
					},
					"source_git_linked_properties_typeProperties_url": {
						"value": "[parameters('source_git_linked_properties_typeProperties_url')]"
					}
